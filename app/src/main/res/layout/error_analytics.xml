<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/customErrorAnalysisLayout"
     >
    
	 <LinearLayout 
    	android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="5dp"
        android:id="@+id/shotInfoLayout"
        >
        
	    <TextView
	    	android:id="@+id/shotDestination"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	        android:layout_weight="0.4"/>
    
	    <TextView
	        android:id="@+id/shotHand"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	    	android:duplicateParentState="true"/>
	    
	     <TextView
	        android:id="@+id/countTitle"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.2"
			android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	    	android:duplicateParentState="true"/>
  
	</LinearLayout>
	 <LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:id="@+id/displayInfo"
         android:visibility="gone"
         >
	    <TextView
	        android:id="@+id/shotDestinationInfoName"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	        android:layout_weight="0.4"/> 
        
	    <TextView
	        android:id="@+id/shotHandInfoName"
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	    	android:duplicateParentState="true"/>
	    
	    <TextView
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.2"
			android:layout_gravity="center_horizontal"  
			android:gravity="center_horizontal"
	    	android:duplicateParentState="true"/>
	</LinearLayout>
	<LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:visibility="gone"
         android:id="@+id/netInfoLayout"
         >
     
           <TextView
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:gravity="center_horizontal"
			android:text="@string/net"/>
	    	<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
				android:gravity="center_horizontal"
				android:text="@string/colonSeparator"
	     	/>
		    <TextView
		        android:id="@+id/netShotPlayed"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.4"
		     /> 
		     <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.2"
		     />  
     </LinearLayout>
     <LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:visibility="gone"
         android:id="@+id/missInfoLayout"
         >
     
           <TextView
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:gravity="center_horizontal"
			android:text="@string/miss"
     	/>
	    	<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
				android:gravity="center_horizontal"
				android:text="@string/colonSeparator"
	     	/>
		    <TextView
		        android:id="@+id/missShotPlayed"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.4"
		     />  
		      <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.2"
		     />  
     </LinearLayout>
      <LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:visibility="gone"
         android:id="@+id/outInfoLayout"
         >
     
           <TextView
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:gravity="center_horizontal"
			android:text="@string/out"
     	/>
	    	<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
				android:gravity="center_horizontal"
				android:text="@string/colonSeparator"
	     	/>
		    <TextView
		        android:id="@+id/outShotPlayed"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.4"
		     />  
		      <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.2"
		     />  
     </LinearLayout>
        <LinearLayout 
         android:layout_width="fill_parent"
         android:layout_height="wrap_content"
         android:orientation="horizontal"
         android:visibility="gone"
         android:id="@+id/batEdgeInfoLayout"
         >
     
           <TextView
	        android:layout_width="0dp"
	        android:layout_height="wrap_content"
	        android:textSize="14sp"
	        android:textColor="@color/hightlightColor"
	        android:padding="5dp"
	        android:layout_weight="0.4"
			android:gravity="center_horizontal"
			android:text="@string/batedge"
     	/>
	    	<TextView
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
				android:gravity="center_horizontal"
				android:text="@string/colonSeparator"
	     	/>
		    <TextView
		        android:id="@+id/batEdgeShotPlayed"
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.4"
		     />  
		      <TextView
		        android:layout_width="0dp"
		        android:layout_height="wrap_content"
		        android:textSize="14sp"
		        android:textColor="@color/hightlightColor"
		        android:padding="5dp"
		        android:layout_weight="0.2"
		     />  
     </LinearLayout>
</LinearLayout>
